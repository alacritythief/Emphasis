<h1><%= @page.name %></h1>
<h3>page <%= @page.number %></h3>

<% @page.elements.each do |element| %>
  <p><img src="<%= element.image_url %>"></p>
<% end %>

<p><%= link_to 'Create a Element', new_comic_element_path(@comic.name, page: @page.number) %></p>

<p><%= link_to "Back to #{@comic.name}", comic_path(@comic.name) %></p>
